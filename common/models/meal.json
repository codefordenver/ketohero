  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "foods": {
      "type": "hasMany",
      "model": "food",
      "foreignKey": "mealId",
      "through": "mealfood"
    },
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "mealId",
      "through": "usermeal"
    }
  },
  "acls": [],
  "methods": {}
}
